.my-item{
	border-bottom: 1rpx solid #ddd;
	padding: 20rpx;
	font-size: 32rpx;
	background: #fff url('../../images/xiangyou1.png') no-repeat 98% 50% / auto 36rpx;
}
.my-item image{
	width: 40rpx;
	height: 40rpx;
	margin-right: 20rpx;
	vertical-align: -6rpx;
}
page {
  background-color: #f1f1f1;
}

.level {
  display: flex;
  align-items: center;
}

/* 状态栏 */
.bar-box {
  background-color: #2979ff;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 999;
  padding: 20rpx 0;
}

.bar {
  width: 80%;
  margin-right: 30%;
  margin-top: 1.5%;
  margin-left: 20rpx;
}

.bar-title {
  margin-top: 1.5%;
  margin-left: 20rpx;
  text-align: center;
  font-size: 34rpx;
  color: white;
}

.top-head {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  background-color: white;
  margin-right: 30rpx;
}

.head-right {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.welcome {
  margin-bottom: 15rpx;
}

.nick {
  font-size: 36rpx;
  font-weight: bold;
  color: white;
  margin-bottom: 10rpx;
}

.acount {
  font-size: 22rpx;
  margin-top: 5rpx;
}
.avatar-wrapper .avatar {
  margin: 0 auto;
  text-align: center;
}
.head-box {
  background-color: #F9AF55;
  padding: 0 20rpx;
}

.set-center {
  background-color: #19be6b;
  color: white;
  font-size: 24rpx;
  padding: 6rpx 20rpx;
  border-radius: 30rpx;
  align-self: flex-start;
  margin-top: 10rpx;
}

.share-box {
  background-color: rgb(65, 64, 64);
  border-radius: 16rpx;
  padding: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.money {
  font-size: 30rpx;
  color: rgb(248, 201, 70);
}

.money-text {
  padding-left: 20rpx;
}

.share-text {
  font-size: 24rpx;
  color: rgba(248, 212, 112, 0.479);
  margin-top: 8rpx;
}

.share-btn {
  background-color: rgb(248, 201, 70);
  color: rgb(54, 54, 54);
  border-radius: 30rpx;
  font-size: 26rpx;
  padding: 8rpx 24rpx;
}

.sign-box {
  margin: 0 20rpx;
  padding: 30rpx 20rpx;
  font-size: 30rpx;
color: gray;
text-align: center;
  justify-content: space-around;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  background-color: white;
}
/* 二级菜单 */
.tab-box {
  margin: 20rpx;
  background-color: white;
  box-shadow: 0 0 5rpx 5rpx #f1f1f1;
  padding: 30rpx 20rpx;
  border-radius: 20rpx;
}

.tab-list {
  flex: 1;
  text-align: center;
  color: gray;
}

.tab-img {
  width: 50%;
}

.tab-text {
  font-size: 30rpx;
  margin: 5rpx 0;
}

.white-box {
  background-color: white;
  margin: 20rpx;
  border-radius: 20rpx;
  padding: 0 20rpx;
}

.row {
  display: flex;
  align-items: center;
  padding: 36rpx 10rpx;
  font-size: 30rpx;
  font-weight: inherit;
  height: 120rpx;
  background-color: rgba(0, 0, 0, 0);
  border-bottom: 1rpx solid #f1f1f1;
}

.row::after {
  border: none;
}

.text {
  margin-left: 15rpx;
  color: #636262;
}

.left {
  width: 90%;
  text-align: left;
  display: flex;
  align-items: center;
  color: rgb(59, 59, 59);
}

.left image{
  align-self: center;
  width: 35rpx;
  height: 35rpx;
}

.right {
  width: 70%;
  text-align: right;
  color: rgb(148, 147, 147);
}

/* 商标 */
.copy {
  padding: 30rpx;
  text-align: center;
  color: #000000;
}

.copy text {
  font-size: 25rpx;
  color: gray
}

.openid-wrapper {
  display: flex;
  align-items: center;
  background: rgba(255, 255, 255, 0.1);
  padding: 6rpx 12rpx;
  border-radius: 8rpx;
  display: inline-flex;
}

.id-wrapper {
  color: rgba(255, 255, 255, 0.8);
  font-size: 24rpx;
  margin-right: 10rpx;
}

.openid {
  color: white;
  font-size: 24rpx;
  margin-right: 10rpx;
}

.copy {
  color: rgba(255, 255, 255, 0.8);
  font-size: 24rpx;
  background: rgba(255, 255, 255, 0.2);
  padding: 2rpx 10rpx;
  border-radius: 6rpx;
}

.pricetitle {
  text-align: center;
  font-size: 40rpx;
  font-weight: bold;
  margin: 30rpx;
}

.pricelist {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  margin: 20rpx;
}

.priceitem {
  width: 45%;
  background-color: white;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
}

.priceitem image {
  width: 100%;
  height: 200rpx;
  border-radius: 10rpx;
}

.pricename {
  font-size: 30rpx;
  margin: 10rpx 0;
}

.multi-tips{
color: #999;
font-size: 24rpx;
text-align: center;
position: relative; 
}

.container {
  min-height: 100vh;
  background-color: #f5f5f5;
  padding: 20rpx;
}

.user-info {
  display: flex;
  align-items: flex-start;
  margin-bottom: 40rpx;
}

.avatar-container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.avatar {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
  margin-bottom: 20rpx;
}

.user-id {
  color: #fff;
  font-size: 28rpx;
  margin-bottom: 10rpx;
}

.copy-text {
  font-size: 24rpx;
  color: #fff;
  margin-left: 10rpx;
  padding: 4rpx 12rpx;
  border: 1px solid #fff;
  border-radius: 20rpx;
}

.admin-tag {
  background-color: #00cc66;
  color: #fff;
  font-size: 24rpx;
  padding: 4rpx 16rpx;
  border-radius: 20rpx;
}

.info-list {
  background-color: #fff;
  border-radius: 20rpx;
  padding: 20rpx;
  margin-bottom: 20rpx;
}

.info-item {
  display: flex;
  align-items: center;
  padding: 20rpx;
  border-bottom: 1rpx solid #f5f5f5;
}

.info-item:last-child {
  border-bottom: none;
}

.item-icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.item-label {
  color: #666;
  font-size: 28rpx;
  width: 160rpx;
}

.item-value {
  flex: 1;
  color: #333;
  font-size: 28rpx;
}

.version-info {
  background-color: #fff;
  border-radius: 20rpx;
  padding: 20rpx;
  margin-bottom: 20rpx;
}

.version-title {
  font-size: 28rpx;
  color: #666;
  margin-bottom: 20rpx;
  display: block;
}

.version-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: #333;
  padding: 10rpx 0;
}

/* 退出登录按钮 */
.logout-btn {
  background-color: #ff4d4f;
  color: #fff;
  text-align: center;
  padding: 20rpx 0;
  border-radius: 10rpx;
  font-size: 32rpx;
  margin: 40rpx 0;
}

.footer {
  text-align: center;
  color: #999;
  font-size: 24rpx;
  padding: 20rpx 0;
}

.white-box-head {
  background-color: #f7982a;
  margin: 20rpx;
  border-radius: 20rpx;
  padding: 40rpx 30rpx;
}

.info-right {
  flex: 1;
  display: flex;
  flex-direction: column;
}